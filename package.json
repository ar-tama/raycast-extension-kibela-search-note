{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-kibela-note",
  "title": "Search Kibela Note",
  "description": "",
  "icon": "icon_k.png",
  "author": "shimewtr",
  "license": "MIT",
  "commands": [
    {
      "name": "search_notes",
      "title": "Search Kibela Notes",
      "description": "Template with a plain detail view",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "subDomain",
      "type": "textfield",
      "required": true,
      "title": "Kibela sub domain",
      "description": "Sub domain of youre kibela",
      "placeholder": "URL of your GitLab instance e.g. https://gitlab.com"
    },
    {
      "name": "accessToken",
      "type": "password",
      "required": true,
      "title": "Access Token",
      "description": "Your Kibela Access Token",
      "link": "https://my.kibe.la/settings/access_tokens",
      "placeholder": "Enter your Kibela API Key"
    }
  ],
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@raycast/api": "^1.30.2",
    "cross-fetch": "^3.1.5",
    "graphql": "^16.3.0",
    "node-fetch": "^3.2.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
